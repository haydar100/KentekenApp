<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".kentekenMain">

    <EditText
        android:id="@+id/getMerk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Sent"
        android:layout_centerHorizontal="true"
        android:text="merk" />

    <EditText
        android:id="@+id/vulKenteken"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/getMerk"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="68dp"
        android:text="kentekenzonderstreepjes" />

    <Button
        android:id="@+id/Sent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:onClick="OK"
        android:text="Verstuur" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/vulKenteken"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/vulKenteken"
        android:layout_marginTop="88dp" />

    <EditText
        android:id="@+id/merkenType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/getMerk"
        android:layout_centerHorizontal="true"
        android:text="merkenType" />

</RelativeLayout>
