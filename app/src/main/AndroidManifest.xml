<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.berkan.kentekenapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".kentekenMain"
            android:label="@string/app_name">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CarDetailActivity"
            android:label="@string/title_activity_car_detail"
            android:parentActivityName=".kentekenMain"></activity>
        <activity
            android:name=".ListKentekensActivity"
            android:label="@string/title_activity_list_kentekens"></activity>
    </application>

</manifest>
